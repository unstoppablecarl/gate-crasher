{
    "name": "unstoppablecarl/gate-crasher",
    "description": "Safer Laravel superuser auth",
    "type": "library",
    "keywords": [
        "laravel",
        "auth",
        "gate",
        "superuser",
        "policy"
    ],
    "homepage": "https://github.com/unstoppablecarl/gate-crasher",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Olsen",
            "email": "unstoppablecarlolsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/config": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "illuminate/support": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "illuminate/contracts": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*"
    },
    "require-dev": {
        "illuminate/auth": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*",
        "phpunit/phpunit": "^5.7 || ^6 || ^7",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "UnstoppableCarl\\GateCrasher\\": "src",
            "UnstoppableCarl\\GateCrasherExamples\\": "examples"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnstoppableCarl\\GateCrasher\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/unstoppablecarl/gate-crasher/issues",
        "source": "https://github.com/unstoppablecarl/gate-crasher"
    },
    "suggest": {
        "unstoppablecarl/arbiter": "Manage Laravel User abilities that target Users"
    },
    "scripts": {
        "phpunit": "phpunit",
        "phpcs": "phpcs src tests --standard=psr2 -sp --colors",
        "test": [
            "@phpunit",
            "@phpcs"
        ]
    }
}
